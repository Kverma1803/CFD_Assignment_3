FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * *x * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    diamond
    {
    	type		zeroGradient;	
    	}
    outlet
    {
        type            fixedValue;
        value		uniform 0;
    }
    walls
    {
    	type		zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

